<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.component.bottomnavigation.BottomNavigationNewsFragment">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/picture_news"
        android:title="@string/bottom_navigation_picture_news"
        android:scaleType="centerCrop"
        android:contentDescription="@string/app_bar_bottom_image_description_news" />

</FrameLayout>
